{
    "name": "randle-discord",
    "description": "Dice, cards, and polling bot for Discord.",
    "version": "1.0.0",
    "main": "./dist/app.js",
    "scripts": {
        "build": "tsc",
        "lint": "eslint ./src/ --fix",
        "start": "node --enable-source-maps ./dist/app.js",
        "dev": "ts-node-dev --inspect --poll --interval 1000 --respawn --rs --transpile-only --watch -r dotenv/config ./src/app.ts"
    },
    "engines": {
        "node": "^16"
    },
    "dependencies": {
        "discord.js": "github:discordjs/discord.js",
        "express": "^4.17.1",
        "ordinal": "^1.0.3"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.1",
        "@types/express": "^4.17.12",
        "@types/node": "^14.14.6",
        "@typescript-eslint/eslint-plugin": "^4.6.0",
        "@typescript-eslint/parser": "^4.6.0",
        "dotenv": "^8.2",
        "eslint": "^7.12.1",
        "ts-node-dev": "^1.0.0-pre.62",
        "typescript": "^4.1.3"
    },
    "license": "MIT",
    "author": {
        "name": "Arcanigma Foundation",
        "email": "randle@arcanigma.org",
        "url": "https://github.com/arcanigma"
    },
    "repository": "github:arcanigma/randle-discord",
    "private": true
}
